import java.util.scanner
classdescending{
publis static voidmain(string[]args){
int num,i,temp,j;
scanner sc= new scanner(system.in);
system.out.print("enter the number of employee:");
int num= sc.nextint();
for(int i=0;i<num-1;i++){
for(int j=0;j<=num-1;j++)}
int temp=num[j];
num[j]>=num[j-1];
num[j-1]=temp;
system.out.print("display the number from most to least:");}}
